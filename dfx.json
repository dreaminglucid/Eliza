{
    "canisters": {
        "agent_framework": {
            "type": "assets",
            "source": [
                "dist"
            ],
            "build": [
                "pnpm install",
                "pnpm build:ic"
            ],
            "declarations": {
                "output": "declarations"
            }
        }
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": ""
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        },
        "ic": {
            "providers": [
                "https://mainnet.dfinity.network"
            ],
            "type": "persistent"
        }
    },
    "version": 1,
    "dfx": "0.21.0"
}
{
    "canisters": {
        "agent_framework": {
            "type": "azle",
            "main": "ic/index.ts",
            "custom": {
                "experimental": true,
                "candid_gen": "http",
                "assets": [
                    [
                        "dist",
                        "dist"
                    ]
                ],
                "build_assets": "pnpm build:ic"
            }
        }
    },
    "defaults": {
        "build": {
            "args": "",
            "packtool": ""
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:8000",
            "type": "ephemeral"
        },
        "ic": {
            "providers": [
                "https://mainnet.dfinity.network"
            ],
            "type": "persistent"
        }
    },
    "version": 1,
    "dfx": "0.21.0"
}